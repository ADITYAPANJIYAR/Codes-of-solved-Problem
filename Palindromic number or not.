#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int temp=n,sum=0,r;
    
    while(n>0)
    {
    	r=n%10;
    	sum=(sum*10)+r;
    	n=n/10;
    }
    
    if(sum==temp)
    cout<<"the number is palindrome"<<endl;
    else
    cout<<"the number is not palindrome"<<endl;
}
