#include<iostream>
#include <stdio.h>
using namespace std;

int main()
{
    int i,j,t;
    cin>>t;
    
    for(j=0;j<t;j++){
        int a,b,a1,b1,a2,b2,sum=0;
        cin>>a>>b>>a1>>b1>>a2>>b2;
        
        if( (a1==a and b1==b) or(a1==b and b1==a)){
            cout<<1<<endl;
        }
        else if( (a2==a and b2==b) or(a2==b and b2==a)){
            cout<<2<<endl;
        }
        else{
            cout<<0<<endl;
        }
        
    }//testcase loop...
}
