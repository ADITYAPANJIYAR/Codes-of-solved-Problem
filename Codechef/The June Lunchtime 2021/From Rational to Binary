#include<bits/stdc++.h>
#include <stdio.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    long long int t,i,j;
    cin>>t;
    for(j=0;j<t;j++){
        long long int a,b;
        cin>>a>>b;
    if(floor(log2(b))== ceil(log2(b))){
        printf("YES\n");
    }
    else{
        printf("NO\n");
    }
        
    }
}
