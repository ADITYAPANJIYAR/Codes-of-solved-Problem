#include <bits/stdc++.h>
#include <stdio.h>
using namespace std;

int main()
{
    int i,j,t;
    cin>>t;
    for(j=0;j<t;j++){
        int n;
        cin>>n;
        int a[n];
        
        for(i=0;i<n;i++){
            cin>>a[i];
            if(a[i]%2==0){
                cout<<a[i]<<" ";
            }
        }
        
        for(i=0;i<n;i++){
            if(a[i]%2!=0){
                cout<<a[i]<<" ";
            }
        }
        cout<<endl;
        
    }//testcase loop...
}
