#include<bits/stdc++.h>
#include <stdio.h>
using namespace std;

int main()
{
    int i,j,k,t;
    cin>>t;
    
    for(j=0;j<t;j++){
        int a[11]={0},sum=0;
        
        for(i=0;i<4;i++){
            int x;
            cin>>x;
            a[x]=a[x]+1;
        }
        
        
        for(i=0;i<11;i++){
            if(a[i]==4){
                cout<<0<<endl;
                sum=sum+1;
                break;
            }
            else if(a[i]==3){
                cout<<1<<endl;
                sum=sum+1;
                break;
            }
            else if(a[i]==2){
                cout<<2<<endl;
                sum=sum+1;
                break;
            }
        }
        
        if(sum==0){
            cout<<2<<endl;
        }
        
        
    }//loop...
    
    
}
