#include<bits/stdc++.h>
#include <stdio.h>
using namespace std;

int main()
{
    int i,j,t;
    cin>>t;
    
    for(j=0;j<t;j++){
        int a,b,c;
        int d,e,f;
        cin>>a>>b>>c;
        cin>>d>>e>>f;
        a=a+b+c;
        d=d+e+f;
        if(a>d){
            cout<<1<<endl;
        }
        else{
            cout<<2<<endl;
        }
    }
}
