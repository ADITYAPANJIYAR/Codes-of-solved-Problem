#include <bits/stdc++.h>
using namespace std;

int main() 
{
	// your code goes here
	int t;
	cin>>t;
	while(t)
	{
	    t--;
	    int n,m,k;
	    cin>>n>>m>>k;
	    int a[n];
	    for(int i=0;i<n;i++)
	    cin>>a[i];
	    
	    int flag=1;
	    for(int i=0;i<n;i++)
	    {
	        if((a[i]+k-1)>m)
	        {    
	            flag=0;
	            break;
	        }
	    }
	    if(flag)
	    cout<<"YES"<<endl;
	    else
	    cout<<"NO"<<endl;
	}
}
