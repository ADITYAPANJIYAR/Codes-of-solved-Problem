#include<bits/stdc++.h>
using namespace std;

int main()
{
    int i,j,t,k;
    cin>>t;
    
    for(j=0;j<t;j++){
        int a,b,c;
        cin>>a>>b>>c;
        int left=0,right=0;
        
        if(a==0){
            left++;
        }
        else{
            right++;
        }
        
        if(b==0){
            left++;
        }
        else{
            right++;
        }
        
        if(c==0){
            left++;
        }
        else{
            right++;
        }
        
        if(left>0 and right>0){
            cout<<1<<endl;
        }
        else{
            cout<<0<<endl;
        }
        
    }//testcase loop...
}
