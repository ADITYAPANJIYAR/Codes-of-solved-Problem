class Solution
{
	public:
    int gcd(int a, int b) 
	{ 
	    // code here
	    int ans;
	    while(a!=b)
	    {
	        if(a>b)
	        {
	            a=a-b;
	        }
	        else
	        {
	            b=b-a;
	        }
	    }
	    ans=a;
	    ans=b;//this will also work.
	    return ans;
	} 
};
