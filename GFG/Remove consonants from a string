class Solution{
    public:
    string removeConsonants(string s)
    {
        //complete the function heredef removeConsonants(s):
        string ans;
        for(int i=0;i<s.size();i++)
        {
            if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' ||s[i]=='u' || s[i]=='A'|| s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U')
            ans=ans+s[i];
        }
        if(ans=="")
        return "No Vowel";
        return ans;
        
    }
};
