class Solution{   
public:
    string isVowel(char c){
        // code here 
        string s;
        if(c=='a' or c=='e' or c=='i' or c=='o' or c=='u'){
            s="YES";
            return s;
        }
        else if(c=='A' or c=='E' or c=='I' or c=='O' or c=='U'){
            s="YES";
            return s;
        }
        else{
            s="NO";
            return s;
        }
    }
};
